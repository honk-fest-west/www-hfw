---
import Title from "@components/Title.astro";
import allstarsTile from "@images/bands/allstars.jpg";
---

<div class="flex flex-col w-full h-full">
  <Title title="HONK!" subtitle="All Stars" />
  <div class="rounded-lg overflow-hidden">
    <a
      href="/allstars"
      class="w-full h-full block min-h-80 bg-contain bg-repeat-x bg-center"
      style=`background-image: url("${allstarsTile.src}");`
    >
    </a>
  </div>
</div>
