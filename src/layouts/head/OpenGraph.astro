---
import socialmediaImg from "@assets/images/2025/2025-opengraph.png";
import { getOpenGraphStartTime, getOpenGraphEndTime } from '@util/dates';
---

<meta property="og:url" content="https://honkfestwest.org/" />
<meta property="og:title" content="HONK! Fest West" />
<meta
    property="og:description"
    content="An independent 501(c)(3) arts organization that produces an annual free, three-day, community-supported music festival in public spaces of the Seattle area."
/>
<meta property="og:image" content={socialmediaImg.src} />
<meta property="og:image:width" content={`${socialmediaImg.width}`} />
<meta property="og:image:height" content={`${socialmediaImg.height}`} />
<meta property="og:type" content="event" />
<meta property="og:event_type" content="Festival" />
<meta property="og:start_time" content={getOpenGraphStartTime()} />
<meta property="og:end_time" content={getOpenGraphEndTime()} />
<meta property="og:location" content="Seattle, WA" />
<meta property="og:street_address" content="Varies throughout Seattle" />

<meta property="event:start_time" content={getOpenGraphStartTime()} />
<meta property="event:end_time" content={getOpenGraphEndTime()} />
<meta
    property="event:venue"
    content="Seattle, WA (Fri: Georgetown, Sat: Columbia City Park, Sun: TBD)"
/>
<meta
    property="event:organizer"
    content="https://www.facebook.com/honkfestwest"
/>
<meta property="event:published_time" content="2023-10-26T00:00:00-07:00" />
